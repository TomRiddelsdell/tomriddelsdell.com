  "scripts": {
    "dev": "NODE_ENV=development tsx interfaces/api-gateway/src/index.ts",
    "build": "vite build && esbuild interfaces/api-gateway/src/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:lambda": "esbuild interfaces/api-gateway/src/aws-lambda-adapter.ts --platform=node --target=node18 --bundle --format=esm --external:aws-sdk --external:@aws-sdk/* --outfile=dist/lambda/index.mjs --sourcemap",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:mcp": "tsx scripts/test-mcp-servers.ts",
    "security": "npm audit --audit-level=high",
    "security:check": "npm audit --audit-level=moderate --json",
    "security:report": "npm audit --audit-level=moderate > security-audit-$(date +%Y%m%d).txt",
    "aws:deploy": "./infrastructure/deployment/aws/scripts/deploy.sh",
    "aws:deploy:dev": "./infrastructure/deployment/aws/scripts/deploy.sh development",
    "aws:deploy:staging": "./infrastructure/deployment/aws/scripts/deploy.sh staging",
    "aws:deploy:prod": "./infrastructure/deployment/aws/scripts/deploy.sh production",
    "aws:setup-cicd": "./infrastructure/deployment/aws/scripts/setup-github-actions.sh"
